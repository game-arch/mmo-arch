{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "allowJs": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2015",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2018",
            "dom",
            "scripthost"
        ],
        "types": [
            "phaser"
        ],
        "paths": {
            "server-list": [
                "dist/server-list"
            ],
            "server-list/*": [
                "dist/server-list/*"
            ],
            "authentication": [
                "dist/authentication"
            ],
            "authentication/*": [
                "dist/authentication/*"
            ],
            "server-selection": [
                "dist/server-selection"
            ],
            "server-selection/*": [
                "dist/server-selection/*"
            ],
            "connection": [
                "dist/connection"
            ],
            "connection/*": [
                "dist/connection/*"
            ],
            "character-selection": [
                "dist/character-selection"
            ],
            "character-selection/*": [
                "dist/character-selection/*"
            ],
            "player": [
                "dist/player"
            ],
            "player/*": [
                "dist/player/*"
            ],
            "game-engine": [
                "dist/game-engine"
            ],
            "game-engine/*": [
                "dist/game-engine/*"
            ]
        }
    },
    "angularCompilerOptions": {
        "fullTemplateTypeCheck": true,
        "strictInjectionParameters": true
    },
    "exclude": ["../server/node_modules"]
}
